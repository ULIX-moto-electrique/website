---
import PageLayout from "../layouts/PageLayout.astro";

import SliderAccueil from "../components/SliderAccueil.astro";
import SectionAbout from "../components/SectionAbout.astro";
import SectionProjets from "../components/SectionProjets.astro";
import SectionPartenaires from "../components/SectionPartenaires.astro";
import SectionComite from "../components/SectionComite.astro";

import global_settings from "../data/global_settings.json";
import accueil from "../data/accueil.json";
import slider from "../data/slider.json";
import comite from "../data/comite.json";
---

<PageLayout title={ global_settings.title }>


  <SliderAccueil
    title={ accueil.slider_title }
    subtitle={ accueil.slider_subtitle }
    slider_img={ slider }
  />

  <SectionAbout
    title={ accueil.a_propos_title }
    content={ accueil.a_propos_content }
  />

  <SectionPartenaires
    title={ accueil.partenaires_title }
    content={ accueil.partenaires_content }
    btn={ accueil.partenaires_btn }
  />

  <SectionComite
    title={ accueil.comite_title }
    members={ comite.members }
  />


</PageLayout>
